Algoritmo "notas"

Var
   nota1, nota2, notaf: real


Inicio
      escreva("Digite a primeira nota: ")
      leia(nota1)
      escreva("Digite a segunda nota: ")
      leia(nota2)
      notaf <- nota1 + nota2
      escreval("NOTA FINAL =", notaf:5:1)
      se (notaf < 60) entao
      escreval("REPROVADO")
      fimSe




Fimalgoritmo